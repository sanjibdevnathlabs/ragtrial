[run]
relative_files = true
source = .
omit = 
    */tests/*
    */venv/*
    */models/*
    */storage/*
    */htmlcov/*
    */migration/versions/*
    */migration/templates/*
    */scripts/*
    */examples/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

