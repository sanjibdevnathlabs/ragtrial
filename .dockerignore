venv/
.venv/
ENV/
env/

# Testing
.pytest_cache/
.coverage
htmlcov/
.tox/
.mypy_cache/
*.log

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Git
.git/
.gitignore
.gitattributes

# CI/CD
.github/
.gitlab-ci.yml

# Documentation (not needed in container)
*.md
!README.md
docs/
examples/

